  "version": "0.2.0",
  "configurations": [

    {
      "name": "on_time_front",
      "request": "launch",
      "type": "dart",
      "args": [
        "--dart-define-from-file",
        ".env/debug.env",
        "--web-port",
        "58102"

      ]
    },
    {
      "name": "on_time_front (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "args": [
        "--dart-define-from-file",
        ".env/profile.env"
      ]
    },
    {
      "name": "on_time_front (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "args": [
        "--dart-define-from-file",
        ".env/release.env"
      ]
    },
    {
      "name": "assets",
      "cwd": "assets",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "widgetbook",
      "cwd": "widgetbook",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "widgetbook (profile mode)",
      "cwd": "widgetbook",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "widgetbook (release mode)",
      "cwd": "widgetbook",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}